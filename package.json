{
  "name": "mywatcho",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "gulp --tenant=timesplay --env=beta && next dev",
    "herogotv:beta": "gulp --tenant=herogotv --env=beta && next dev",
    "herogotv:prod": "gulp --tenant=herogotv --env=prod && next dev",
    "timesplay:beta": "gulp --tenant=timesplay --env=beta && next dev",
    "aggregator:beta": "gulp --tenant=aggregator --env=beta && next dev",
    "timesplay:prod": "gulp --tenant=timesplay --env=prod && next dev",
    "build": "gulp --tenant=timesplay --env=beta && next build",
    "herogotv:build:prod": "gulp --tenant=herogotv --env=prod && next build",
    "start": "next start",
    "lint": "next lint",
    "aastha": "npm run dev --tenant=aastha",
    "firstshows": "npm run dev --tenant=firstshows",
    "watcho": "npm run dev --tenant=watcho",
    "prepare": "husky && husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "~1.9.5",
    "axios": "^1.6.2",
    "axios-cache-interceptor": "~1.3.2",
    "cookie": "~0.5.0",
    "eslint": "~8.33.0",
    "eslint-config-next": "~13.5.6",
    "js-cookie": "~3.0.1",
    "next": "~13.5.6",
    "nextjs-progressbar": "~0.0.16",
    "node-fetch": "~3.3.2",
    "qs": "~6.11.1",
    "react": "~18.2.0",
    "react-datepicker": "^4.25.0",
    "react-dom": "~18.2.0",
    "react-helmet": "~6.1.0",
    "react-hook-form": "~7.43.9",
    "react-otp-input": "^3.1.1",
    "react-redux": "~8.0.7",
    "react-slick": "~0.29.0",
    "slick-carousel": "~1.8.1"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.3.1",
    "@types/cookie": "~0.5.1",
    "@types/js-cookie": "~3.0.3",
    "@types/node": "~20.10.5",
    "@types/qs": "~6.9.7",
    "@types/react-datepicker": "^4.19.5",
    "@types/react-dom": "~18.2.18",
    "@types/react-helmet": "~6.1.6",
    "@types/react-redux": "~7.1.33",
    "@types/react-slick": "~0.23.10",
    "gulp": "^4.0.2",
    "gulp-clean": "~0.4.0",
    "gulp-delete-file": "~1.0.2",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "sass": "~1.58.0",
    "typescript": "~5.3.3",
    "yargs": "~17.7.2"
  },
  "lint-staged": {
    "**/*": "prettier .src/ --write --ignore-unknown"
  }
}
